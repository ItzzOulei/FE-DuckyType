    "materialize-css": "^1.0.0-rc.2",
    "next": "15.2.4",
    "nodemon": "^3.1.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "cypress": "^14.2.1",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.2.4"
  }
}
